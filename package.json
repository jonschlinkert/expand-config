{
  "name": "normalize-config",
  "description": "Normalize a declarative configuration with any combination of src-dest mappings, files arrays, files objects and options into a consistent format. Uses globule to expand glob patterns and rte for dynamically generating destination paths. Inspired by Grunt's `normalizeMultiTaskFiles` method.",
  "version": "0.3.1",
  "homepage": "https://github.com/jonschlinkert/normalize-config",
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jonschlinkert/normalize-config.git"
  },
  "bugs": {
    "url": "https://github.com/jonschlinkert/normalize-config/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/jonschlinkert/normalize-config/blob/master/LICENSE-MIT"
    }
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha -R spec"
  },
  "dependencies": {
    "globule": "^0.2.0",
    "lodash": "^2.4.1",
    "normalize-path": "^0.1.0",
    "rte": "^0.1.4"
  },
  "devDependencies": {
    "chai": "~1.9.1",
    "fs-utils": "^0.4.3",
    "mocha": "*",
    "plasma": "^0.4.0",
    "verb": "~0.2.6",
    "verb-tag-jscomments": "^0.1.4"
  },
  "keywords": [
    "config",
    "configuration",
    "dest",
    "destination",
    "expand",
    "files array",
